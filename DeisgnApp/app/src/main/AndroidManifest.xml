<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deisgnapp">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DeisgnApp">
        <activity android:name=".widget.RadioButtonActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".widget.SeekBarActivity">

        </activity>
        <activity android:name=".widget.SpinnerActivity"></activity>
        <activity android:name=".widget.RatingBarActivity" />

        <service
            android:name=".servicewithnotification.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".servicewithnotification.ServicesNOtificationActivity" />
        <activity android:name=".callingapi.SmSActivity" />
        <activity android:name=".callingapi.CallingActivity" />
        <activity android:name=".widget.DialogActivity" />
        <activity android:name=".widget.CheckbBoxActivity" />
        <activity android:name=".materialdesign.recyclerview.RecyclerviewActivity" />
        <activity
            android:name=".materialdesign.tabslayout.TabActivity"
            android:label="@string/title_activity_tab"
            android:theme="@style/Theme.DeisgnApp.NoActionBar" />
        <activity
            android:name=".materialdesign.bootamneivitaion.BootamNevigationActivity"
            android:label="@string/title_activity_bootam_nevigation" />
        <activity
            android:name=".materialdesign.NavigationActivity"
            android:label="@string/title_activity_navigation"
            android:theme="@style/Theme.DeisgnApp.NoActionBar" />
        <activity android:name=".acitivtylifecycle.TwoActivity" />
        <activity android:name=".acitivtylifecycle.OneActivity" />
        <activity
            android:name=".fragment.WhatsUpActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".intentdata.ImplicitActivity" />
        <activity android:name=".intentdata.SecondActivity" />
        <activity android:name=".intentdata.FirstActivity" />
        <activity android:name=".intentdata.HomeActivity" />
        <activity
            android:name=".intentdata.SignInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>